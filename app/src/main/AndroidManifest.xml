<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".di.App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/img_yourweatherlogo"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/img_yourweatherlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Yourweather"
        tools:targetApi="31">
        <activity
            android:name=".presentation.calendardetailview.CalendarDetailViewStatusbar"
            android:exported="false" />
        <activity
            android:name="com.kakao.sdk.auth.AuthCodeHandlerActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="oauth"
                    android:scheme="${KAKAO_API_KEY}" />
            </intent-filter>
        </activity>

        
        <activity
            android:name=".presentation.calendar.CalendarDate"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.calendar.CalendarDetail"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".presentation.calendar.CalendarMonth"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".presentation.calendardetailview.CalendarDetailViewAlarm"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.calendardetailview.CalendarDetailViewEmpty"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.calendardetailview.CalendarDetailViewTimepicker"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.calendardetailview.CalendarModifyWeatherActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.calendardetailview.CalendarPlusWeather"
            android:exported="true"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".presentation.calendardetailview.CalendarWeatherDetail"
            android:exported="true"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />

        <activity
            android:name=".presentation.mypage.MyPageAlarmActivity"
            android:exported="true"
            android:screenOrientation="portrait" />

        <activity
            android:name=".presentation.mypage.MyPageMyInfoActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.mypage.MyPageNicknameChangeActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.mypage.MyPagePlus"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.mypage.MyPagePlusAlarm"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.mypage.MyPageUsePolicy"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.mypage.MyPagePrivacyPolicy"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.mypage.MyPageWithdarw3"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.mypage.MyPageWithdraw2"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.mypage.MyPageWithdraw"
            android:exported="true"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.mypage.MyPagePwChangeActivity"
            android:exported="false"
            android:screenOrientation="portrait" />

        <activity
            android:name=".presentation.share.SharedViewModel"
            android:exported="true"
            android:screenOrientation="portrait" />

        <activity
            android:name=".presentation.sign.FindPwEmailActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.sign.FindPwActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.sign.ResetPwActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
