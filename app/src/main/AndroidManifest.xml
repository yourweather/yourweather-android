<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/img_yourweatherlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/img_yourweatherlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Yourweather"
        tools:targetApi="31">
        <activity
            android:name=".presentation.UnwrittenDetailListActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.WrittenDetailListActivity"
            android:exported="false"
            android:label="@string/title_activity_written_detail_list"
            android:theme="@style/Theme.Yourweather" />
        <activity
            android:name=".presentation.AnalysisActivity"
            android:exported="false" />
        <activity
            android:name=".presentation.UnwrittenDetailListActivity"
            android:exported="false"
            tools:ignore="DuplicateActivity" />
        <activity
            android:name=".presentation.SignUp2"
            android:exported="false" />
        <activity
            android:name=".presentation.Nickname"
            android:exported="false" />
        <activity
            android:name=".presentation.SignUp"
            android:exported="false" />
        <activity
            android:name=".presentation.SignIn"
            android:exported="false" />
        <activity
            android:name=".presentation.FindPw"
            android:exported="false" />
        <activity
            android:name=".presentation.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.Splash"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>