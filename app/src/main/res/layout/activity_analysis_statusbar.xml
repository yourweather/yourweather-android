<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
<androidx.appcompat.widget.Toolbar
        android:id="@+id/tb_analysis_back"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="672dp"
        android:background="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:context=".presentation.analysis.AnalysisStatusbar">

        <RelativeLayout
            android:id="@+id/rl_analysis1"
            android:layout_width="26dp"
            android:layout_height="wrap_content">
            <ImageButton
                android:id="@+id/rl_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:background="#00ff0000"
                app:srcCompat="@drawable/btn_calendardetailview_back" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_analysis2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">



            <TextView
                android:id="@+id/tv_analysis_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text="@string/tv_Unwritten_title"
                android:textColor="@color/sorange"
                android:textSize="20dp"
                android:textStyle="bold"
                android:gravity="center"
                android:layout_marginEnd="134dp"
                android:layout_marginStart="114dp"
                android:layout_marginBottom="16dp"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/bmhanna"/>
        </RelativeLayout>

    </androidx.appcompat.widget.Toolbar>
</androidx.constraintlayout.widget.ConstraintLayout>